{
  "name": "covid-numbers",
  "version": "0.7.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:prod": "ng test --configuration=ci",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildui": "rmdir public & ng build --watch",
    "install-all": "npm i --prefix ./functions & npm i",
    "install-build-all": "npm run build --prefix ./functions & npm run build",
    "ib-all": "npm run install-build-all",
    "emulate": "firebase emulators:start --only functions,firestore,hosting",
    "deploy": "ng build --prod & firebase deploy --only hosting",
    "deployall": "firebase deploy",
    "postdeploy": "npm run release",
    "release": "standard-version -a",
    "release-dry": "standard-version -a --dry-run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/cdk": "~9.2.4",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/fire": "^6.0.0-rc.1",
    "@angular/forms": "~9.1.9",
    "@angular/material": "~9.2.4",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@mszewcz/safe-subscribe": "^1.0.21",
    "@ngx-translate/core": "^12.0.0",
    "@ngx-translate/http-loader": "^5.0.0",
    "@swimlane/ngx-charts": "^13.0.2",
    "cookieconsent": "^3.1.1",
    "firebase": "^7.15.0",
    "firebase-admin": "^9.0.0",
    "firebase-functions": "^3.9.0",
    "ngx-cookieconsent": "^2.2.3",
    "ngx-papaparse": "^4.0.4",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.900.0-0 || ^0.900.0",
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~9.1.9",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "firebase-tools": "^8.7.0",
    "fuzzy": "^0.1.3",
    "husky": "^4.2.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ngx-translate-testing": "^4.0.0",
    "prettier": "2.0.1",
    "pretty-quick": "^2.0.1",
    "protractor": "^7.0.0",
    "puppeteer": "^4.0.0",
    "standard-version": "^9.1.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
